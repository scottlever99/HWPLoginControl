<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HWPLoginControl.styles.css" asp-append-version="true" />
    <link rel="icon" href="@Url.Content("~/muscleicon.ico")" />
</head>
<body class="pagebackground">
    <header>
        <nav class="navbar navbar-light navbar-background box-shadow mb-3 fw-bold">
            <div class="container-fluid justify-content-center">
                <a class="navbar-brand" href="https://hwpfitness.com/"><img src="~/images/hwplogo.png" style="height:5rem;" /></a>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3"
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
